{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.sap.refapps.shine.web",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {
			"default": {
				"uri": "/odata/v2/EmployeeService/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.sap.refapps.shine.web.view.main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.refapps.shine.web.i18n.i18n"
				}
			},
			"": {
				"preload": true,
				"dataSource": "default",
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"groupId": "$direct",
					"synchronizationMode": "None"
				},
				"type": "sap.ui.model.odata.v2.ODataModel"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sap.refapps.shine.web.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Routemain",
				"pattern": "Routemain",
				"target": [
					"Targetmain"
				]
			}, {
				"pattern": "detail/{employeePath}",
				"name": "detail",
				"target": "detail"
			}],
			"targets": {
				"Targetmain": {
					"viewType": "XML",
					"clearControlAggregation": false,
					"viewName": "main",
					"viewId":"main",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "com.sap.refapps.shine.web.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"detail": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "detail",
					"viewId": "detail",
					"viewPath": "com.sap.refapps.shine.web.view"
				}
			}
		}
	}
}